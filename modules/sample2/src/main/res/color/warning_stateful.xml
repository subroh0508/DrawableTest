<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:color="@color/warning_disabled"/>
    <item android:state_hovered="true" android:color="@color/warning_hovered"/>
    <item android:state_pressed="true" android:color="@color/warning_pressed"/>
    <item android:state_activated="true" android:color="@color/warning_activated"/>
    <item android:state_focused="true" android:color="@color/warning_focused"/>
    <item android:state_selected="true" android:color="@color/warning_selected"/>
    <item android:color="@color/warning"/>
</selector>